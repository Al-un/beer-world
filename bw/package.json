{
  "name": "@al-un/bw-html",
  "version": "1.0.0",
  "description": "Vanilla HTML, JS and SCSS implementation of Beerworld",
  "scripts": {
    "dev": "webpack serve --config webpack.dev.js --open",
    "build:lib": "NODE_ENV=production webpack --config webpack.lib.js",
    "build:site": "NODE_ENV=production webpack --config webpack.prod.js",
    "lint:code": "eslint",
    "lint:code:all": "eslint --ext .ts,.js --ignore-pattern 'webpack.*.js' .",
    "lint:style": "stylelint",
    "lint:style:all": "stylelint \"**/*.{sass,scss}\"",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Al-un/beerworld.git"
  },
  "author": "Al-un",
  "license": "ISC",
  "homepage": "https://github.com/Al-un/beerworld#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@storybook/addon-actions": "^6.1.15",
    "@storybook/addon-essentials": "^6.1.15",
    "@storybook/addon-links": "^6.1.15",
    "@storybook/html": "^6.1.15",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.3.3",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-order": "^4.1.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1"
  }
}
